{
  "name": "nodejs-express-boilerplate-v2",
  "version": "1.0.0",
  "description": "boilerplate app intergrated with elasticsearch and sql",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon -r dotenv-flow/config ./src/index.js",
    "start": "cross-env NODE_ENV=production node -r dotenv-flow/config ./build/server.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress",
    "lint": "./node_modules/.bin/eslint .",
    "test:run": "cross-env NODE_ENV=test mocha -r dotenv-flow/config",
    "test:pop": "cross-env NODE_ENV=development node -r dotenv-flow/config contrib/index.js"
  },
  "author": "Charith Jayawardana",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.11.5",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-webpack-plugin": "^2.5.2",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.2.1",
    "webpack": "^5.27.2",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/register": "^7.12.13",
    "@babel/runtime": "^7.13.10",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "casual": "^1.6.2",
    "core-js": "^3.9.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv-flow": "^3.2.0",
    "elasticsearch": "^16.7.2",
    "express": "^4.17.1",
    "extendable-error-class": "^0.1.2",
    "fs-extra": "^9.0.1",
